
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>History</title>
</head>
  <body>
    <h1>History</h1>
    <ul>
      <% allHistory.forEach(entry => { %>
      <li>
        <strong>User id: <%= entry.user_id %></strong> <br />
        <strong><%= entry.request_type %></strong> - <%= entry.outcome %> - <%=
        entry.timestamp %>
      </li>
      <% }); %>
    </ul>

    <a href="/history?downloadPDF=true" target="_blank">Download as PDF</a>
  </body>
</html>